{
  "name": "firestore-utils-cli",
  "version": "1.1.4",
  "description": "Firestore utilily cli to import data",
  "author": "Farsab Butt",
  "bin": {
    "firestore-utils": "./bin/run"
  },
  "homepage": "https://github.com/farsabbutt/firestore-utils-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "farsabbutt/firestore-utils-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.13.10",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "firebase-admin": "^11.0.1",
    "inquirer": "^8.0.0",
    "log-symbols": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "firestore-utils",
    "dirname": "firestore-utils",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/farsabbutt/firestore-utils-cli/issues",
  "keywords": [
    "oclif",
    "firestore",
    "firestore collection export",
    "firestore export cli"
  ],
  "types": "dist/index.d.ts"
}
